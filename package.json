{
  "name": "website_manager",
  "version": "0.1.0",
  "private": true,
  "description": "Version 2 of the Website Manager for howardt12345.com",
  "author": "Howard Tseng",
  "build": {
    "appId": "com.howardt12345.website_manager_2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@firebase/app": "^0.6.5",
    "@reach/router": "^1.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "concurrently": "^5.2.0",
    "electron-is-dev": "^1.2.0",
    "firebase": "^7.15.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "wait-on": "^5.0.1"
  },
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "electron-dev": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0",
    "react-app-rewire-alias": "^0.1.6",
    "react-app-rewired": "^2.1.6"
  }
}
